		"test": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.4.0",
		"@playwright/experimental-ct-svelte": "^1.32.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2",
		"@sveltejs/adapter-static": "^2",
		"@sveltejs/kit": "^1.20.5",
		"@tanstack/svelte-query": "5.0.0-beta.35",
		"@tanstack/svelte-query-devtools": "5.0.0-beta.35",
		"@typescript-eslint/eslint-plugin": "^6",
		"@typescript-eslint/parser": "^6",
		"@testing-library/jest-dom": "^6",
		"@testing-library/svelte": "^4",
		"@vitest/ui": "^0.34",
		"axios": "^1.3.4",
		"bulma": "^0.9.4",
		"dayjs": "^1.11.8",
