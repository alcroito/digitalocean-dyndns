{
	"name": "ddns",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"apigen": "openapi-zod-client ../../crates/dyndns/generated/openapi.json -o ./src/generated/zodiusClient.ts --default-status auto-correct",
		"build": "vite build",
		"watch": "vite build -w",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.4.0",
		"@sveltejs/adapter-auto": "^2",
		"@sveltejs/adapter-static": "^2",
		"@sveltejs/kit": "^1.18.0",
		"@tanstack/svelte-query": "^4.29.7",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"bulma": "^0.9.4",
		"dayjs": "^1.11.7",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"openapi-zod-client": "^1.6.4",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rxjs": "^7.8.1",
		"sass": "^1.62.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tslib": "^2.5.1",
		"typescript": "^5",
		"vite": "^4.3.8"
	},
	"type": "module"
}
