{
	"name": "ddns",
	"version": "0.6.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"apigen": "openapi-zod-client ../../crates/dyndns/generated/openapi.json -o ./src/generated/zodiosClient.ts --default-status auto-correct",
		"build": "vite build",
		"watch": "vite build -w",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.7.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.10.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tanstack/svelte-query": "5.32.0",
		"@tanstack/svelte-query-devtools": "5.32.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"bulma": "^1.0.2",
		"dayjs": "^1.11.13",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"openapi-zod-client": "^1.18.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"rollup-plugin-postcss": "^4.0.2",
		"rxjs": "^7.8.1",
		"sass": "^1.82.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.0"
	},
	"type": "module"
}
