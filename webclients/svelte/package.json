{
	"name": "ddns",
	"version": "0.6.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"apigen": "openapi-zod-client ../../crates/dyndns/generated/openapi.json -o ./src/generated/zodiosClient.ts --default-status auto-correct",
		"build": "vite build",
		"watch": "vite build -w",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.4.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2",
		"@sveltejs/kit": "^1.25.0",
		"@tanstack/svelte-query": "5.32.0",
		"@tanstack/svelte-query-devtools": "5.32.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"bulma": "^0.9.4",
		"dayjs": "^1.11.8",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.38.0",
		"openapi-zod-client": "^1.11.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"rollup-plugin-postcss": "^4.0.2",
		"rxjs": "^7.8.1",
		"sass": "^1.68.0",
		"svelte": "^4.2.1",
		"svelte-check": "^3.7.0",
		"tslib": "^2.5.3",
		"typescript": "^5.2.2",
		"vite": "^4.5.3"
	},
	"type": "module"
}
