		"openapi-zod-client": "^1.7.1",
		"prettier": "^3",
		"prettier-plugin-svelte": "^3",
		"rollup-plugin-postcss": "^4.0.2",
		"rxjs": "^7.8.1",
		"sass": "^1.63.6",
		"svelte": "^4",
		"svelte-check": "^3.4.4",
		"tslib": "^2.5.3",
		"typescript": "^5",
		"vite": "^4.3.9",
		"vitest": "^0.34"
